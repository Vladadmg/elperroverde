<h2>Temas</h2>
<div class='widget-content'>
<div id='labelCloud'></div>
<script type='text/javascript'>

// Don't change anything past this point -----------------
function s(a,b,i,x){
if(a>b){
var m=(a-b)/Math.log(x),v=a-Math.floor(Math.log(i)*m)
}
else{
var m=(b-a)/Math.log(x),v=Math.floor(Math.log(i)*m+a)
}
return v
}

var ta=0
var c=[];
var labelCount = new Array();
var ts = new Object;

var theName = "+-";
ts[theName] = 1;

var theName = "24";
ts[theName] = 1;

var theName = "Anime";
ts[theName] = 2;

var theName = "Análisis";
ts[theName] = 7;

var theName = "Blogueando";
ts[theName] = 16;

var theName = "Chorradas";
ts[theName] = 40;

var theName = "Cine";
ts[theName] = 6;

var theName = "Citas";
ts[theName] = 4;

var theName = "Comics";
ts[theName] = 6;

var theName = "Cuestionarios";
ts[theName] = 1;

var theName = "Descargas";
ts[theName] = 13;

var theName = "Emuladores";
ts[theName] = 6;

var theName = "EOL";
ts[theName] = 2;

var theName = "Flash";
ts[theName] = 2;

var theName = "Frikadas";
ts[theName] = 18;

var theName = "Guitar Hero";
ts[theName] = 11;

var theName = "Ilusiones";
ts[theName] = 2;

var theName = "Imagenes";
ts[theName] = 16;

var theName = "Informática";
ts[theName] = 12;

var theName = "Internet";
ts[theName] = 25;

var theName = "Linux";
ts[theName] = 1;

var theName = "Memes";
ts[theName] = 1;

var theName = "Mitos";
ts[theName] = 2;

var theName = "Musica";
ts[theName] = 28;

var theName = "nVidia";
ts[theName] = 1;

var theName = "Only Japón";
ts[theName] = 7;

var theName = "PC";
ts[theName] = 16;

var theName = "Pensamientos";
ts[theName] = 7;

var theName = "Prison Break";
ts[theName] = 1;

var theName = "Series";
ts[theName] = 6;

var theName = "Street Fighter";
ts[theName] = 4;

var theName = "Tutoriales";
ts[theName] = 8;

var theName = "Videojuegos";
ts[theName] = 86;

var theName = "Videos";
ts[theName] = 43;

var theName = "Wii";
ts[theName] = 4;


for (t in ts){
if (!labelCount[ts[t]]){
labelCount[ts[t]] = new Array(ts[t])
}
}
tz = labelCount.length-1;
lc2 = document.getElementById('labelCloud');
ul = document.createElement('ul');
ul.className = 'label-cloud';
for(var t in ts){
for (var i=0;3 > i;i++) {
c[i]=s(minColor[i],maxColor[i],ts[t]-ta,tz)
}
var fs = s(minFontSize,maxFontSize,ts[t]-ta,tz);
li = document.createElement('li');
li.style.fontSize = fs+'px';
li.style.lineHeight = '1';
a = document.createElement('a');
a.title = ts[t]+' Artículos en '+t;
a.style.color = 'rgb('+c[0]+','+c[1]+','+c[2]+')';
a.href = lcBlogURL+'/search/label/'+encodeURIComponent(t);
if (lcShowCount){
span = document.createElement('span');
span.innerHTML = '('+ts[t]+') ';
span.className = 'label-count';
a.appendChild(document.createTextNode(t));
li.appendChild(a);
li.appendChild(span);
}
else {
a.appendChild(document.createTextNode(t));
li.appendChild(a);
}
ul.appendChild(li);
abnk = document.createTextNode(' ');
ul.appendChild(abnk);
}
lc2.appendChild(ul);
</script>
<noscript>
